  field(SIOL, "$(DAQMX):$(PORT)$(CHAN):DATA:SIM PP")
}

record(ao, "$(DAQMX):$(PORT)$(CHAN):DATA:SIM"){
    field(DESC, "Output data for RECSIM")
}
